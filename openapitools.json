{
  "$schema": "./node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "7.8.0",
    "generators": {
      "v2.0": {
        "generatorName": "aspnetcore",
        "output": "../cs-oauth-provider-lib",
        "inputSpec": "./cs-oauth-provider-lib-swagger.yaml",
        "additionalProperties": {
          "apsnetCoreVersion": "8.0",
          "packageName": "OAuthProviderLib",
          "buildTarget": "library",
          "sourceFolder": "./src"
        }
      }
    }
  }
}